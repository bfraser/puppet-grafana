# This file is managed by Puppet, any changes will be overwritten

<%- @cfg.keys.sort.each do |key|
  if @cfg[key].is_a?(Hash) -%>

[<%= key %>]
    <%- @cfg[key].keys.sort.each do |valkey| -%>
<%= valkey %> = <%= @cfg[key][valkey] %>
    <%- end -%>
  <%- else -%>
<%= key %> = <%= @cfg[key] %>
  <%- end
end -%>
